                                <tipoActuacion></tipoActuacion>
                                <fechasActuacion></fechasActuacion>
                            </alcanceActuacion>
                        </actuador>
                    </actuadores>
                    <sensores idSensor="">
                        <numeroSensores></numeroSensores>
                        <sensor>
                            <elementoSensado></elementoSensado>
                            <rango></rango>
                        </sensor>
                    </sensores>
                </configuracionDron>
            </dron>
            <identificacion codigoMision="">
            </identificacion>
            <sensores idSensor="">
                <numeroSensores></numeroSensores>
            </sensores>
            <actuadores matriculaDron="">
                <nuemroActuadores></nuemroActuadores>
            </actuadores>
        </ide:descripcion>
    </tns:identificacion>
    <tns:drones>
        <dr:dron matriculaDron="">
            <dr:claseDron></dr:claseDron>
        </dr:dron>
    </tns:drones>
    <tns:tareas>
        <tns:tarea>
            <descripcionTarea></descripcionTarea>
        </tns:tarea>
    </tns:tareas>
</tns:mision>