            xsi:schemaLocation="http://www.piat.upm.es/misionSchema ../Fase%202/misionSchema.xsd">
    <tns:identificacion codigoMision="M123ac">
        <ide:descripcion>
            <tarea>
                <descripcionTarea></descripcionTarea>
                <dronesTarea>
                    <matricula></matricula>
                    <actividadMision></actividadMision>
                </dronesTarea>
                <areaGeografica>
                    <centro>
                        <longitud></longitud>
                        <latitud></latitud>
                    </centro>
                    <profundidad></profundidad>
                    <forma>esfera</forma>
                </areaGeografica>
            </tarea>
            <dron matriculaDron="">
                <claseDron></claseDron>
                <homologaciones>
                    <nacional></nacional>
                    <internacional></internacional>
                </homologaciones>
                <configuracionDron>
                    <actuadores matriculaDron="">
                        <nuemroActuadores></nuemroActuadores>
