<?xml version="1.0" encoding="UTF-8"?>
<tns:mision xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:ide="http://www.piat.upm.es/identificacionSchema"
            xmlns:dr="http://www.piat.upm.es/dronSchema"
            xmlns:sen="http://www.piat.upm.es/sensoresSchema"
            
